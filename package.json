{
  "name": "google-auth-library",
  "version": "0.9.4",
  "author": "Google Inc.",
  "description": "Google APIs Authentication Client Library for Node.js",
  "contributors": [
    {
      "name": "Jason Allor",
      "email": "jasonall@google.com"
    },
    {
      "name": "Tim Emiola",
      "email": "temiola@google.com"
    }
  ],
  "engines": {
    "node": ">=0.10"
  },
  "main": "./lib/auth/googleauth",
  "repository": {
    "type": "git",
    "url": "https://github.com/google/google-auth-library-nodejs.git"
  },
  "keywords": [
    "google",
    "api",
    "google apis",
    "client",
    "client library"
  ],
  "dependencies": {
    "async": "~0.9.0",
    "gtoken": "^1.1.0",
    "jws": "~3.0.0",
    "request": "~2.51.0",
    "string-template": "~0.2.0"
  },
  "devDependencies": {
    "coveralls": "^2.11.1",
    "istanbul": "~0.3.2",
    "keypair": "~1.0.0",
    "js-beautify": "^1.5.1",
    "jshint": "^2.5.5",
    "jsdoc": "~3.3.0-alpha9",
    "mkdirp": "~0.5.0",
    "mocha": "^1.8.1",
    "nock": "~0.46.0",
    "rimraf": "^2.2.8",
    "swig": "^1.4.2",
    "url": "~0.10.1",
    "minimist": "^1.1.0"
  },
  "scripts": {
    "lint": "jshint lib test",
    "test": "mocha --reporter spec --timeout 4000",
    "generate-apis": "node scripts/generate",
    "generate-docs": "jsdoc -c jsdoc-conf.json ./README.md",
    "prepare": "npm run generate-apis && npm test && npm run lint && npm version patch",
    "coverage": "istanbul cover -x 'apis/**' _mocha -- --reporter spec --timeout 4000",
    "coveralls": "istanbul cover -x 'apis/**' _mocha --report lcovonly -- --reporter spec --timeout 4000 && cat coverage/lcov.info | coveralls"
  },
  "license": "Apache-2.0"
}
